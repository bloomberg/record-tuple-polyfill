{
  "name": "@bloomberg/record-tuple-polyfill",
  "version": "0.0.5",
  "description": "A polyfill for the Record and Tuple Stage 2 ECMAScript proposal.",
  "type": "commonjs",
  "main": "lib/index.umd.js",
  "exports": {
      ".": {
          "require": "./lib/index.umd.js",
          "default": "./lib/index.esm.js"
      },
      "./modify-global": {
          "require": "./lib/modify-global.umd.js",
          "default": "./lib/modify-global.esm.js"
      }
  },
  "files": [ "lib/*.js" ],
  "scripts": {
    "clean": "rm -rf ./lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bloomberg/record-tuple-polyfill"
  },
  "keywords": [
    "record",
    "tuple",
    "record and tuple",
    "polyfill",
    "ecmascript"
  ],
  "author": "Rick Button <rbutton2@bloomberg.net>",
  "contributors": [
    "Robin Ricard <rricard2@bloomberg.net>",
    "Nicol√≤ Ribaudo <nribaudo1@bloomberg.net>"
  ],
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  }
}
